@model IEnumerable<HostelBuzz.Models.HostelDetails>  
@{  
     
    ViewBag.Title = "Hostel Details";  
}  
   
<table class="table">  
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.HostelName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HostelAddress)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HostelPhone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HostelEmailID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ContactPerson)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.State)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.City)
        </th>
    </tr>  
    @foreach (var item in Model) {  
        <tr>  
            <td>  
                @Html.DisplayFor(modelItem=>item.HostelName)  
            </td>  
            <td>  
                @Html.DisplayFor(modelItem=> item.HostelAddress)  
            </td>  
            <td>  
                @Html.DisplayFor(modelItem => item.HostelPhone)  
            </td>  
            <td>  
                @Html.DisplayFor(modelItem => item.HostelEmailID)  
            </td>  
            <td>  
                @Html.DisplayFor(modelItem => item.ContactPerson)  
            </td>  
            <td>  
                @Html.DisplayFor(modelItem => item.State)  
            </td>  
            <td>  
                @Html.DisplayFor(modelItem => item.City)  
            </td>  
            <td>  
                @Html.ActionLink("Edit", "Edit", new { id = item.HostelID }) |  
                @Html.ActionLink("Details", "Details", new { id = item.HostelID }) |  
                @Html.ActionLink("Delete", "Delete", new { id = item.HostelID })  
            </td>  
        </tr>  
    }         
</table>  